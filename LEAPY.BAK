//program to check whether the entered year is leap year or not

#include<stdio.h>
#include<conio.h>

void main()
{
   int year;
   clrscr();
   printf("\nenter year");
   scanf("%d",&year);
   if(year%4==0)
      printf("\nleap year");
  else
      printf("\nnot leap year");
   getch();
}