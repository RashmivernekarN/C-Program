//program to check whether the entered character is vowel or not
#include<stdio.h>
#include<conio.h>
void main()
{
    char ch;
    clrscr();
    printf("\n\tenter any character\t");
    scanf("%c",&ch);
    switch(ch)
    {
      case 'a':
      case 'A':
      case 'e':
      case 'E':
      case 'i':
      case 'I':
      case 'o':
      case 'O':
      case 'u':
      case 'U':printf("\n\tENTERED CHARACTER IS VOWEL");
	       break;
      default:printf("\nENTERED CHARACTER IS NOT VOWEL");
	      break;
      }
      getch();
}

