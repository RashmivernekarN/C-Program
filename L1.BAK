//display odd up to n  using while

#include<stdio.h>
#include<conio.h>

void main()
{
    int i,n;
    clrscr();
    printf("\n\t enter final value");
    scanf("%d",&n);
    printf("\n\tODD NUMBERs UP TO %d are\n",n);
    i=1;
    while(i<=n)
    {
       printf("\t%d\n",i);
       i=i+2;
     }
     getch();
}
