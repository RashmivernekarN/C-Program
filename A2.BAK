//prg to read a matrix of order 3x3 and print the transpose of given matrix
#include<stdio.h>
#include<conio.h>
void main()
  {
    int a[3][3],r,c;
    clrscr();
    printf("\n Enter the elements of the matrix");
    for(r=0;r<3;r++)
      {
	for(c=0;c<3;c++)
	 {
	  scanf("%d",&a[r][c]);
	 }
      }
    printf("\n  Transpose of the matrix is:\n ");
