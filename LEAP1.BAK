//program to check whether the entered year is leap year or not

#include<stdio.h>
#include<conio.h>

void main()
{
   int year;
   clrscr();
   printf("\n\tenter year");
   scanf("%d",&year);
   if(year%4==0)
       printf("\n\t%d is leap year",year);
   else
      printf("\n\t%d is NOT leap year",year);
   getch();
}